select microsecond(adddate(dtm, interval id microsecond)), microsecond(adddate(dtm, interval id*id microsecond)) from dtypes;
select id, vc30, MICROSECOND(vc30) from dtypes;
select id, vc30 from dtypes where MICROSECOND(vc30) = id;
select id, vc30 from dtypes where MICROSECOND(vc30)<> id;
select id, vc30 from dtypes where MICROSECOND(vc30) = vc255;
SELECT MICROSECOND('12:00:00.123456');
SELECT MICROSECOND('2009-12-31 23:59:59.000010');