select cidx, CDATE, LAST_DAY(CDATE) from datatypetestm;
select cidx, CDATETIME, LAST_DAY(CDATETIME) from datatypetestm;
select cidx, CDATE from datatypetestm where LAST_DAY('2004-02-06') > CDATE;
select cidx, CDATETIME from datatypetestm where LAST_DAY('2004-02-06') > CDATETIME;
select cidx, CDATE from datatypetestm where LAST_DAY(CDATE) <> CDATE;
select cidx, CDATE from datatypetestm where LAST_DAY(CDATE) = CDATE;
select cidx, CDATETIME from datatypetestm where LAST_DAY(CDATETIME) <> CDATETIME;
select cidx, CDATETIME from datatypetestm where LAST_DAY(CDATETIME) = CDATETIME;